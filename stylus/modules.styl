	&__item_active &__header
		box-shadow 0 1px 5px rgba(22, 20, 17, 0.16)

	&__item_active &__header:after
		transform rotate(180deg)

// Mixins
alert-style( color )
	border 1px solid color + 50%
	background color + 90%
	.alert
		&__title
		&__close
		&__body
			color color - 20%

		&__close
			svg
				fill color
			&:hover
				svg
					fill color - 40%

